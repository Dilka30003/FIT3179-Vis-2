{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 200,
    "title": "Number of Earthquakes in New Zealand",
    "data": {
        "url": "data/earthquakes_nz.csv"
    },
    "mark": "line",
    "encoding": {
        "x": {
            "title": "Year",
            "timeUnit": "year",
            "field": "origin_time"
        },
        "y": {
            "title": "Number of Earthquakes",
            "aggregate": "count",
            "field": "preffered_magnitude",
            "type": "quantitative"
        }
    }
}